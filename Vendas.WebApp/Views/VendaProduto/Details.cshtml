@model Vendas.WebApp.Models.ViewModels.VendaProdutoFormViewModels
@{
    ViewData["Title"] = "Detalhar venda produto";
}
<h2 class="display-4">Detalhar venda produto</h2>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id
        </dt>
        <dd>
            @Html.DisplayFor(model => model.VendaProduto.Id)
        </dd>
        <dt>
            VendaId
        </dt>
        <dd>
            @Html.DisplayFor(model => model.VendaProduto.VendaId)
        </dd>
        <dt>
            ProdutoId
        </dt>
        <dd>
            @Html.DisplayFor(model => model.VendaProduto.ProdutoId)
        </dd>
        <dt>
            Produto
        </dt>
        <dd>
            @Html.DisplayFor(model => model.VendaProduto.Produto.Nome)
        </dd>
        <dt>
            Quantidade
        </dt>
        <dd>
            @Html.DisplayFor(model => model.VendaProduto.Quantidade)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class="btn btn-sm btn-secondary">Voltar</a>
</div>