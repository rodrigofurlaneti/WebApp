@model WebApp.Models.Venda
@{
    ViewData["Title"] = "Apagar WebApp";
}
<h2 class="display-4">Apagar venda</h2>
<h5 class="display-4">Tem certeza que deseja apagar a venda id número @Html.DisplayFor(Model => Model.Id)?</h5>
<form asp-action="Delete">
    <input type="submit" value="Apagar" class="btn btn-sm btn-danger" />
</form>
<div>
    <a asp-action="Index" class="btn btn-sm btn-secondary">Voltar</a>
</div>